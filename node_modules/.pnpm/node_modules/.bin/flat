#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/flat@5.0.2/node_modules/flat/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/flat@5.0.2/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules:/Users/lo/wk/lura/_integrations/node_modules:/Users/lo/wk/lura/node_modules:/Users/lo/wk/node_modules:/Users/lo/node_modules:/Users/node_modules:/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules/flat/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/flat@5.0.2/node_modules/flat/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/flat@5.0.2/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules:/Users/lo/wk/lura/_integrations/node_modules:/Users/lo/wk/lura/node_modules:/Users/lo/wk/node_modules:/Users/lo/node_modules:/Users/node_modules:/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules/flat/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../flat/cli.js" "$@"
else
  exec node  "$basedir/../flat/cli.js" "$@"
fi
