#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules/typescript/bin/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules/typescript/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules:/Users/lo/wk/lura/_integrations/node_modules:/Users/lo/wk/lura/node_modules:/Users/lo/wk/node_modules:/Users/lo/node_modules:/Users/node_modules:/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/ts-node@10.8.0_3ffeca41e430ac205105ff1fe4cb5347/node_modules/ts-node/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/ts-node@10.8.0_3ffeca41e430ac205105ff1fe4cb5347/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules/typescript/bin/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules/typescript/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/typescript@4.7.2/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules:/Users/lo/wk/lura/_integrations/node_modules:/Users/lo/wk/lura/node_modules:/Users/lo/wk/node_modules:/Users/lo/node_modules:/Users/node_modules:/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/ts-node@10.8.0_3ffeca41e430ac205105ff1fe4cb5347/node_modules/ts-node/node_modules:/Users/lo/wk/lura/_integrations/structurae-test/node_modules/.pnpm/ts-node@10.8.0_3ffeca41e430ac205105ff1fe4cb5347/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.7.2/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.7.2/node_modules/typescript/bin/tsserver" "$@"
fi
